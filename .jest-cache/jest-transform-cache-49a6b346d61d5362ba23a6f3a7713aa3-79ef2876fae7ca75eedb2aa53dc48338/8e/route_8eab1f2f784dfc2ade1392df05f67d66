    (cov_yjs8xfo7h().s[22]++, await supabase.from('comments').select(`
        likes,
        posts!inner (
          boards (
            board_id,
            name
          )
        )
      `).eq('user_id', user.id).is('deleted_at', null));
    let totalLikes =
    /* istanbul ignore next */
    (cov_yjs8xfo7h().s[23]++, 0);
    const boardCounts =
    /* istanbul ignore next */
    (cov_yjs8xfo7h().s[24]++, {});
    /* istanbul ignore next */
    cov_yjs8xfo7h().s[25]++;
    if (
    /* istanbul ignore next */
    (cov_yjs8xfo7h().b[6][0]++, statsData) &&
    /* istanbul ignore next */
    (cov_yjs8xfo7h().b[6][1]++, !statsError)) {
      /* istanbul ignore next */
      cov_yjs8xfo7h().b[5][0]++;
      cov_yjs8xfo7h().s[26]++;
      statsData.forEach(comment => {
        /* istanbul ignore next */
        cov_yjs8xfo7h().f[2]++;
        cov_yjs8xfo7h().s[27]++;
        totalLikes +=
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[7][0]++, comment.likes) ||
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[7][1]++, 0);
        // Json 객체에서 게시판 이름 추출
        const boardNameObj =
        /* istanbul ignore next */
        (cov_yjs8xfo7h().s[28]++, comment.posts?.boards?.name);
        let boardName =
        /* istanbul ignore next */
        (cov_yjs8xfo7h().s[29]++, null);
        /* istanbul ignore next */
        cov_yjs8xfo7h().s[30]++;
        if (
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[9][0]++, boardNameObj) &&
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[9][1]++, typeof boardNameObj === 'object')) {
          /* istanbul ignore next */
          cov_yjs8xfo7h().b[8][0]++;
          cov_yjs8xfo7h().s[31]++;
          boardName =
          /* istanbul ignore next */
          (cov_yjs8xfo7h().b[10][0]++, boardNameObj.ko) ||
          /* istanbul ignore next */
          (cov_yjs8xfo7h().b[10][1]++, boardNameObj.en) ||
          /* istanbul ignore next */
          (cov_yjs8xfo7h().b[10][2]++, 'Unknown Board');
        } else {
          /* istanbul ignore next */
          cov_yjs8xfo7h().b[8][1]++;
          cov_yjs8xfo7h().s[32]++;
          if (typeof boardNameObj === 'string') {
            /* istanbul ignore next */
            cov_yjs8xfo7h().b[11][0]++;
            cov_yjs8xfo7h().s[33]++;
            boardName = boardNameObj;
          } else
          /* istanbul ignore next */
          {
            cov_yjs8xfo7h().b[11][1]++;
          }
        }
        /* istanbul ignore next */
        cov_yjs8xfo7h().s[34]++;
        if (boardName) {
          /* istanbul ignore next */
          cov_yjs8xfo7h().b[12][0]++;
          cov_yjs8xfo7h().s[35]++;
          boardCounts[boardName] = (
          /* istanbul ignore next */
          (cov_yjs8xfo7h().b[13][0]++, boardCounts[boardName]) ||
          /* istanbul ignore next */
          (cov_yjs8xfo7h().b[13][1]++, 0)) + 1;
        } else
        /* istanbul ignore next */
        {
          cov_yjs8xfo7h().b[12][1]++;
        }
      });
    } else
    /* istanbul ignore next */
    {
      cov_yjs8xfo7h().b[5][1]++;
    }
    const totalBoards =
    /* istanbul ignore next */
    (cov_yjs8xfo7h().s[36]++, Object.keys(boardCounts).length);
    const mostActiveBoard =
    /* istanbul ignore next */
    (cov_yjs8xfo7h().s[37]++, Object.keys(boardCounts).length > 0 ?
    /* istanbul ignore next */
    (cov_yjs8xfo7h().b[14][0]++, Object.entries(boardCounts).sort(([, a], [, b]) => {
      /* istanbul ignore next */
      cov_yjs8xfo7h().f[3]++;
      cov_yjs8xfo7h().s[38]++;
      return b - a;
    })[0][0]) :
    /* istanbul ignore next */
    (cov_yjs8xfo7h().b[14][1]++, null));
    // 7. 데이터 변환
    const transformedComments =
    /* istanbul ignore next */
    (cov_yjs8xfo7h().s[39]++, (
    /* istanbul ignore next */
    (cov_yjs8xfo7h().b[15][0]++, comments) ||
    /* istanbul ignore next */
    (cov_yjs8xfo7h().b[15][1]++, [])).map(comment => {
      /* istanbul ignore next */
      cov_yjs8xfo7h().f[4]++;
      // content가 JSON 형태인 경우 텍스트로 변환
      let contentText =
      /* istanbul ignore next */
      (cov_yjs8xfo7h().s[40]++, '');
      /* istanbul ignore next */
      cov_yjs8xfo7h().s[41]++;
      if (comment.content) {
        /* istanbul ignore next */
        cov_yjs8xfo7h().b[16][0]++;
        cov_yjs8xfo7h().s[42]++;
        if (typeof comment.content === 'string') {
          /* istanbul ignore next */
          cov_yjs8xfo7h().b[17][0]++;
          cov_yjs8xfo7h().s[43]++;
          contentText = comment.content;
        } else {
          /* istanbul ignore next */
          cov_yjs8xfo7h().b[17][1]++;
          cov_yjs8xfo7h().s[44]++;
          if (Array.isArray(comment.content)) {
            /* istanbul ignore next */
            cov_yjs8xfo7h().b[18][0]++;
            cov_yjs8xfo7h().s[45]++;
            // Quill Delta 형식인 경우
            contentText = comment.content.map(op => {
              /* istanbul ignore next */
              cov_yjs8xfo7h().f[5]++;
              cov_yjs8xfo7h().s[46]++;
              return typeof op.insert === 'string' ?
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[19][0]++, op.insert) :
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[19][1]++, '');
            }).join('').replace(/\n+/g, ' ').trim();
          } else {
            /* istanbul ignore next */
            cov_yjs8xfo7h().b[18][1]++;
            cov_yjs8xfo7h().s[47]++;
            if (typeof comment.content === 'object') {
              /* istanbul ignore next */
              cov_yjs8xfo7h().b[20][0]++;
              cov_yjs8xfo7h().s[48]++;
              contentText = JSON.stringify(comment.content);
            } else
            /* istanbul ignore next */
            {
              cov_yjs8xfo7h().b[20][1]++;
            }
          }
        }
      } else
      /* istanbul ignore next */
      {
        cov_yjs8xfo7h().b[16][1]++;
      }
      // 로케일에 따른 게시판 이름 선택
      const locale =
      /* istanbul ignore next */
      (cov_yjs8xfo7h().s[49]++,
      /* istanbul ignore next */
      (cov_yjs8xfo7h().b[21][0]++, searchParams.get('locale')) ||
      /* istanbul ignore next */
      (cov_yjs8xfo7h().b[21][1]++, 'ko'));
      const board =
      /* istanbul ignore next */
      (cov_yjs8xfo7h().s[50]++, comment.posts?.boards);
      let boardName =
      /* istanbul ignore next */
      (cov_yjs8xfo7h().s[51]++, null);
      /* istanbul ignore next */
      cov_yjs8xfo7h().s[52]++;
      if (board?.name) {
        /* istanbul ignore next */
        cov_yjs8xfo7h().b[22][0]++;
        const boardNameObj =
        /* istanbul ignore next */
        (cov_yjs8xfo7h().s[53]++, board.name);
        /* istanbul ignore next */
        cov_yjs8xfo7h().s[54]++;
        if (typeof boardNameObj === 'object') {
          /* istanbul ignore next */
          cov_yjs8xfo7h().b[23][0]++;
          cov_yjs8xfo7h().s[55]++;
          switch (locale) {
            case 'en':
              /* istanbul ignore next */
              cov_yjs8xfo7h().b[24][0]++;
              cov_yjs8xfo7h().s[56]++;
              boardName =
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[25][0]++, boardNameObj.en) ||
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[25][1]++, boardNameObj.ko) ||
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[25][2]++, 'Unknown Board');
              /* istanbul ignore next */
              cov_yjs8xfo7h().s[57]++;
              break;
            case 'ja':
              /* istanbul ignore next */
              cov_yjs8xfo7h().b[24][1]++;
              cov_yjs8xfo7h().s[58]++;
              boardName =
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[26][0]++, boardNameObj.ja) ||
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[26][1]++, boardNameObj.ko) ||
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[26][2]++, 'Unknown Board');
              /* istanbul ignore next */
              cov_yjs8xfo7h().s[59]++;
              break;
            case 'zh':
              /* istanbul ignore next */
              cov_yjs8xfo7h().b[24][2]++;
              cov_yjs8xfo7h().s[60]++;
              boardName =
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[27][0]++, boardNameObj.zh) ||
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[27][1]++, boardNameObj.ko) ||
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[27][2]++, 'Unknown Board');
              /* istanbul ignore next */
              cov_yjs8xfo7h().s[61]++;
              break;
            case 'id':
              /* istanbul ignore next */
              cov_yjs8xfo7h().b[24][3]++;
              cov_yjs8xfo7h().s[62]++;
              boardName =
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[28][0]++, boardNameObj.id) ||
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[28][1]++, boardNameObj.ko) ||
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[28][2]++, 'Unknown Board');
              /* istanbul ignore next */
              cov_yjs8xfo7h().s[63]++;
              break;
            default:
              /* istanbul ignore next */
              cov_yjs8xfo7h().b[24][4]++;
              cov_yjs8xfo7h().s[64]++;
              boardName =
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[29][0]++, boardNameObj.ko) ||
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[29][1]++, boardNameObj.en) ||
              /* istanbul ignore next */
              (cov_yjs8xfo7h().b[29][2]++, 'Unknown Board');
              /* istanbul ignore next */
              cov_yjs8xfo7h().s[65]++;
              break;
          }
        } else {
          /* istanbul ignore next */
          cov_yjs8xfo7h().b[23][1]++;
          cov_yjs8xfo7h().s[66]++;
          if (typeof boardNameObj === 'string') {
            /* istanbul ignore next */
            cov_yjs8xfo7h().b[30][0]++;
            cov_yjs8xfo7h().s[67]++;
            boardName = boardNameObj;
          } else
          /* istanbul ignore next */
          {
            cov_yjs8xfo7h().b[30][1]++;
          }
        }
      } else
      /* istanbul ignore next */
      {
        cov_yjs8xfo7h().b[22][1]++;
      }
      cov_yjs8xfo7h().s[68]++;
      return {
        id: comment.comment_id,
        content: contentText,
        createdAt: comment.created_at,
        postTitle:
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[31][0]++, comment.posts?.title) ||
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[31][1]++, 'No title'),
        postId:
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[32][0]++, comment.posts?.post_id) ||
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[32][1]++, comment.post_id),
        boardName: boardName,
        isAnonymous:
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[33][0]++, comment.posts?.is_anonymous) ||
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[33][1]++, false),
        likeCount:
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[34][0]++, comment.likes) ||
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[34][1]++, 0),
        replyCount:
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[35][0]++, comment.replies) ||
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[35][1]++, 0),
        parentCommentId: comment.parent_comment_id,
        isHidden:
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[36][0]++, comment.is_hidden) ||
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[36][1]++, false),
        locale: comment.locale
      };
    }));
    // 8. 페이지네이션 정보 계산
    const totalPages =
    /* istanbul ignore next */
    (cov_yjs8xfo7h().s[69]++, Math.ceil((
    /* istanbul ignore next */
    (cov_yjs8xfo7h().b[37][0]++, totalCount) ||
    /* istanbul ignore next */
    (cov_yjs8xfo7h().b[37][1]++, 0)) / limit));
    const hasNext =
    /* istanbul ignore next */
    (cov_yjs8xfo7h().s[70]++, page < totalPages);
    const hasPrevious =
    /* istanbul ignore next */
    (cov_yjs8xfo7h().s[71]++, page > 1);
    // 9. 응답 데이터 구성
    const response =
    /* istanbul ignore next */
    (cov_yjs8xfo7h().s[72]++, {
      success: true,
      data: transformedComments,
      pagination: {
        page,
        limit,
        totalCount:
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[38][0]++, totalCount) ||
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[38][1]++, 0),
        totalPages,
        hasNext,
        hasPrevious
      },
      statistics: {
        totalComments:
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[39][0]++, totalCount) ||
        /* istanbul ignore next */
        (cov_yjs8xfo7h().b[39][1]++, 0),
        totalLikes,
        totalBoards,
        mostActiveBoard
      }
    });
    /* istanbul ignore next */
    cov_yjs8xfo7h().s[73]++;
    return _server.NextResponse.json(response);
  } catch (error) {
    /* istanbul ignore next */
    cov_yjs8xfo7h().s[74]++;
    console.error('Comments history fetch error:', error);
    /* istanbul ignore next */
    cov_yjs8xfo7h().s[75]++;
    return _server.NextResponse.json({
      success: false,
      error: 'Server error occurred',
      details: process.env.NODE_ENV === 'development' ?
      /* istanbul ignore next */
      (cov_yjs8xfo7h().b[40][0]++, String(error)) :
      /* istanbul ignore next */
      (cov_yjs8xfo7h().b[40][1]++, undefined)
    }, {
      status: 500
    });
  }
}
