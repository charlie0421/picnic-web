      (cov_1a4sc1sf7a().b[13][0]++, Object.getOwnPropertyDescriptor(obj, key)) :
      /* istanbul ignore next */
      (cov_1a4sc1sf7a().b[13][1]++, null));
      /* istanbul ignore next */
      cov_1a4sc1sf7a().s[26]++;
      if (
      /* istanbul ignore next */
      (cov_1a4sc1sf7a().b[15][0]++, desc) && (
      /* istanbul ignore next */
      (cov_1a4sc1sf7a().b[15][1]++, desc.get) ||
      /* istanbul ignore next */
      (cov_1a4sc1sf7a().b[15][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_1a4sc1sf7a().b[14][0]++;
        cov_1a4sc1sf7a().s[27]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_1a4sc1sf7a().b[14][1]++;
        cov_1a4sc1sf7a().s[28]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_1a4sc1sf7a().b[11][1]++;
    }
  }
  /* istanbul ignore next */
  cov_1a4sc1sf7a().s[29]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_1a4sc1sf7a().s[30]++;
  if (cache) {
    /* istanbul ignore next */
    cov_1a4sc1sf7a().b[16][0]++;
    cov_1a4sc1sf7a().s[31]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_1a4sc1sf7a().b[16][1]++;
  }
  cov_1a4sc1sf7a().s[32]++;
  return newObj;
}
