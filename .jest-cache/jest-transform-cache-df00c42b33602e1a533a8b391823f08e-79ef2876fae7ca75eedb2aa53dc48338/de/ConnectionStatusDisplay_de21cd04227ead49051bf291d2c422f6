  } else
  /* istanbul ignore next */
  {
    cov_1zs0cq2hso().b[6][1]++;
  }
  var newObj =
  /* istanbul ignore next */
  (cov_1zs0cq2hso().s[23]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_1zs0cq2hso().s[24]++,
